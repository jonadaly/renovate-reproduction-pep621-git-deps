{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "packageRules": [
    {
      "matchDepPatterns": [".*"],
      "enabled": false
    },
    {
      "matchDepNames": [
        "httpx",
        "requests"],
      "groupName": "dependencies",
      "enabled": true
    }
  ]
}
